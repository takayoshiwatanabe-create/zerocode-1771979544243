{
  "expo": {
    "name": "スタンプカードアプリ",
    "slug": "myapp",
    "version": "1.2.1",
    "orientation": "portrait",
    "icon": "./assets/app_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zerocode.myapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "buildNumber": "14"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_icon.png",
        "backgroundColor": "#87CEEB"
      },
      "package": "com.zerocode.myapp",
      "versionCode": 6
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "react-native-iap",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3940256099942544~3347511713",
          "iosAppId": "ca-app-pub-3940256099942544~1458002511"
        }
      ],
      "expo-localization"
    ],
    "locales": {
      "ja": "./locales/ja.json",
      "en": "./locales/en.json",
      "zh": "./locales/zh.json",
      "ko": "./locales/ko.json",
      "es": "./locales/es.json",
      "fr": "./locales/fr.json",
      "de": "./locales/de.json",
      "pt": "./locales/pt.json",
      "ar": "./locales/ar.json",
      "hi": "./locales/hi.json"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b676d3f7-3d42-486d-81c4-c9796babec76"
      }
    },
    "owner": "yoshi-san",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#FFFBEB"
    }
  }
}